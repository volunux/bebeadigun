extends ../layout

block content
		
		.art-individual
			.photo-photo-detail
				if true
					img(src ='/photos/a.jpg')
					img(src ='./photos/a.jpg')
			if art
				| !{(art.art_markup).replace(/<p><br><\/p>/g , '').replace(/<div><br><\/div>/g , '<br/>').replace(/<br><br><br>/g , '<br/>')}
				p
					b Ethnic Group : 
					| #{art.ethnic_group.eyon}

			if reference
				for ref in reference
					li #{ref}
